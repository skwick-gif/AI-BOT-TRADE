# AI Trading Bot Environment Configuration
# Copy this file to .env and customize for your setup

# IBKR Configuration (Interactive Brokers)
IBKR_HOST=127.0.0.1
IBKR_PORT=4001  # IB Gateway (4001), TWS Paper (7497), TWS Live (7496)
IBKR_CLIENT_ID=1

# API Keys (get from respective providers)
PERPLEXITY_API_KEY=your_perplexity_api_key_here
FMP_API_KEY=your_fmp_api_key_here
QUANTIQ_API=your_quantiq_api_key_here
FRED_API_KEY=your_fred_api_key_here
NEWSAPI_KEY=your_news_api_key_here

# Reddit API (optional)
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret
REDDIT_USER_AGENT=StockAnalysisBot/1.0

# Application Settings
DEBUG=false
LOG_LEVEL=INFO